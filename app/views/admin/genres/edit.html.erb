<div class="container">
  <div class="row">
    <h4>ジャンル編集</h4>
　</div>
　<div class="row">
    <div class="d-flex flex-row">
      <p class="mx-3">ジャンル名<p>
      <%= form_with model: @genre, url: admin_genre_path, method: :patch do |f|  %>
      <%= f.text_field :name , class: 'mx-3'%>
      <%= f.submit '変更を保存', class: 'btn btn-success' %>
      <% end %>
    </div>
  </div>
</div>